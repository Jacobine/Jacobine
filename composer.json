        }
    ],
    "require": {
        "videlalvaro/php-amqplib": "v2.2.6",
        "kriswallsmith/buzz": "v0.10",
        "symfony/class-loader": "v2.4.0",
        "symfony/yaml": "v2.4.0",
        "symfony/console": "v2.4.0",
        "symfony/finder": "v2.4.0",
        "symfony/dom-crawler": "v2.4.0",
        "phploc/phploc": "v2.0.4",
        "pdepend/pdepend": "1.1.3",
        "monolog/monolog": "1.7.0",
        "andygrunwald/gerrie": "dev-master",
        "pear-pear.php.net/Net_NNTP": "*"
